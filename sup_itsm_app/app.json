{
  "expo": {
    "name": "previc-4d-otrs",
    "slug": "previc-4d-otrs",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/previc_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/previc_splash_2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.devhb.previc-4d-otrs"
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/previc_adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.devhb.previc_4d_otrs"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/previc_favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2ffb3555-3d4f-4db6-9f1b-1b68abbcb1b4"
      }
    },
    "owner": "hbravo"
  }
}